<script type="text/javascript">
   function showMenu(){
       var toogleMenu = document.querySelector('.menubar');
       var navigasi = document.querySelector('.menu');
       toogleMenu.classList.toggle('active')
       navigasi.classList.toggle('active')
   }
</script>
